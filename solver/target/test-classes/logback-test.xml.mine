<configuration debug="false">

    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern><!--%d{HH:mm:ss.SSS} -->%msg%n</pattern>
        </encoder>
    </appender>
    
    <timestamp key="bySecond" datePattern="yyyyMMdd_HHmmss"/>

    <!--<appender name="FILE" class="ch.qos.logback.core.FileAppender">
        <file>GALAK.LOG</file>
        <encoder>
            <pattern>%msg%n</pattern>
        </encoder>
    </appender>-->

    <logger name="choco" level="debug"/>

    <logger name="test" level="debug"/>

    <logger name="debug" level="debug"/>

    <logger name="bench" level="debug"/>

    <logger name="solver" level="debug"/>

    <logger name="solver.search" level="debug"/>

     <logger name="fzn" level="debug"/>

    <root level="warn">
        <appender-ref ref="STDOUT"/>
        <!--<appender-ref ref="FILE"/>-->
    </root>
</configuration>